    f_NBIOT_InitialRegistration_Step5_12(nbiot_Cell1, CONTROL_PLANE, STATE2_NB_CONNECTED_UPDATED, v_NasInd);
    
      
    f_NBIOT_InitialRegistration_Step13(nbiot_Cell1,
