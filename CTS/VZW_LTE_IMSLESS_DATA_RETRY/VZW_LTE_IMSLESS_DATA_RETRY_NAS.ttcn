    }*/
      
       v_PDNType := v_NasInd.Pdu.PiggybackedPduList[0].Msg.pDN_CONNECTIVITY_REQUEST.pdnType.typeValue;
  
    if(match(v_PDNType,tsc_PdnType_IPv4))
